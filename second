#include <stdio.h>
void main()
{
    int a[]={10,20,30,40,50,150,60,80},i,n;
    int l=0,sl=0;
    n=sizeof (a)/sizeof (a[0]);
    printf("size of n %d",n);
    for(i=0;i<n;i++)
    {
        if(a[i]>l)
        {
        l=a[i];
            
        }
        
        else if (a[i]>sl)
        {
            sl<l;
            sl=a[i];
        }
        
    }
    printf("\nlargest element is %d",l);
    printf("\nsecond largest is %d",sl);
}
